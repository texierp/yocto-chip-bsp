<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote fetch="git://git.yoctoproject.org" name="yocto"/>
  <remote fetch="git://git.openembedded.org" name="oe"/>
  <remote fetch="git://github.com" name="github"/>

  <!-- Default use yocto master branch -->
  <default sync-j="4" revision="morty"/>

  <!-- yocto and openembedded -->
  <project remote="yocto" revision="morty" name="poky" path="sources/poky"/>
  <project remote="oe" revision="morty" name="meta-openembedded" path="sources/meta-openembedded"/>

  <!-- Qt5 -->
  <project remote="github" revision="morty" name="meta-qt5/meta-qt5" path="sources/meta-qt5"/>
  
  <!-- chip -->
  <project remote="github" revision="master" name="texierp/meta-chip" path="sources/meta-chip"/>

  <!-- SDK -->
  <project remote="github" revision="master" name="NextThingCo/CHIP-SDK" path="SDK/CHIP-SDK"/>
  
</manifest>

